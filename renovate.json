{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>ydataai/renovate-config"
  ],
  "baseBranches": [
    "develop"
  ],
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "semanticCommitType": "chore",
      "semanticCommitScope": "actions",
      "automerge": false,
      "automergeType": "pr",
      "ignoreTests": false,
      "platformAutomerge": true
    }
  ]
}
